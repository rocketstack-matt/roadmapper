{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/roadmap.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/view.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/view/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/view.js"
    },
    {
      "src": "/api/roadmap/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/roadmap.js"
    }
  ]
}