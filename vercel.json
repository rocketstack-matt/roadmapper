{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/roadmap.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/view.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/embed.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/html.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo.svg",
      "dest": "/public/logo.svg"
    },
    {
      "src": "/rocketstack-matt.png",
      "dest": "/public/rocketstack-matt.png"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/html/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/html.js"
    },
    {
      "src": "/embed/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/embed.js"
    },
    {
      "src": "/view/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/view.js"
    },
    {
      "src": "/(?<owner>[^/]+)/(?<repo>[^/]+)/(?<colorScheme>[^/]+)?",
      "dest": "/api/roadmap.js"
    }
  ]
}